{
    "name": "nemwiper/laravelcaptcha",
    "description": "Create captcha easily with laravel.",
    "keywords": [
        "captcha",
        "audio",
        "validation",
        "languages",
        "laravel"
    ],
    "type": "plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Soprano",
            "email": "wiper@libero.it"
        },
        {
            "name": "Luca Pacitto",
            "email": "lucapacitto.dev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lucbu\\LaravelCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lucbu\\LaravelCaptcha\\CaptchaServiceProvider"
            ]
        }
    },
    "require": {
		"php": ">=8.1.0",
        "falahati/php-mp3": "dev-master",
        "laravel/framework": "^10.0"
	},
	"minimum-stability" : "dev"
}
