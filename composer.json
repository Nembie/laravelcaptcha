{
    "name": "nembie/laravelcaptcha",
    "description": "Create captcha easily with laravel.",
    "keywords": ["captcha", "audio", "validation", "languages", "laravel"],
    "type": "plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Soprano",
            "email": "wiper@libero.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lucbu\\LaravelCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lucbu\\LaravelCaptcha\\CaptchaServiceProvider"
            ]
        }
    },
    "require": {
		"php": ">=7.3.0",
        "falahati/php-mp3": "dev-master"
	},
	"minimum-stability" : "dev"
}
